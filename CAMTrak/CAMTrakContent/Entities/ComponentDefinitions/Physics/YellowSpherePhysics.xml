<?xml version="1.0" encoding="utf-8" ?>
<XnaContent>
  <Asset Type="QuickStart.Components.PhysicsComponentDefinition">
    <ShapeType>Sphere</ShapeType>
    <IsDynamic>true</IsDynamic>    <!-- If ShapeType is 'TriangleMesh', this must be false -->
    <AffectedByGravity>true</AffectedByGravity>
    <Mass>200</Mass>
    <CollisionGroupType>RigidBody</CollisionGroupType>
    <PhysicsModelPath></PhysicsModelPath>    <!-- This is optional, leave empty if physics isn't based on a mesh -->
    <Height>1</Height>    <!-- Used by box, capsule -->
    <Width>1</Width>    <!-- Used by box -->
    <Depth>1</Depth>    <!-- Used by box -->
    <Diameter>1</Diameter>    <!-- Used by sphere, capsule -->
  </Asset>
</XnaContent>
