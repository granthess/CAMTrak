<?xml version="1.0" encoding="utf-8" ?>
<XnaContent>  
  <Asset Type="QuickStart.Components.PhysicsComponentDefinition">
    <!-- The only parameter that Water Physics uses is the 'height', but everything else must still be filled out -->
    <ShapeType>Box</ShapeType>
    <IsDynamic>false</IsDynamic> <!-- If ShapeType is 'TriangleMesh', this must be false -->
    <AffectedByGravity>false</AffectedByGravity>
    <Mass>1</Mass>
    <CollisionGroupType>Phantom</CollisionGroupType>
    <PhysicsModelPath></PhysicsModelPath> <!-- This is optional, leave empty if physics isn't based on a mesh -->
    <Height>300</Height>    <!-- Used by box, capsule -->
    <Width>1</Width>    <!-- Used by box -->
    <Depth>1</Depth>    <!-- Used by box -->
    <Diameter>1</Diameter>    <!-- Used by sphere, capsule -->
  </Asset>
</XnaContent>
